<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="domain.Agent"
           table="agents">
        <id name="ID" column="ID" >
            <generator class="increment"/>
        </id>
        <property name="username" column="usernameu" />
        <property name="password" column="passwordu" />
    </class>
</hibernate-mapping>